{
  "name": "Notifications Preview for GitHub",
  "homepage_url": "https://github.com/tanmayrajani/notifications-preview-github",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Notifications Preview for GitHub with same page pop-over!",
  "applications": {
    "gecko": {
      "id": "notifications-preview-github@tanmayrajani.github.io",
      "strict_min_version": "55.0"
    }
  },
  "icons": {
    "128": "icons/128.png"
  },
  "content_scripts": [{
    "matches": [
      "https://github.com/*"
    ],
    "css": [
      "style.css"
    ],
    "js": [
      "index.js"
    ],
    "run_at": "document_start"
  }]
}
